{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfcopy01"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"TrgGen2Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'TrgGen2Storage'"
		},
		"lnk_src_specific_file_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lnk_src_specific_file_copy'"
		},
		"lnk_trg_speciffilecopy_1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lnk_trg_speciffilecopy_1'"
		},
		"lnk_trg_specific_file_copy_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lnk_trg_specific_file_copy'"
		},
		"TrgGen2Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destsa02.dfs.core.windows.net/"
		},
		"lnk_trg_specific_file_copy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destsa03.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfcopy01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"TrgGen2Storage_accountKey": {
						"value": "[parameters('TrgGen2Storage_accountKey')]"
					},
					"lnk_src_specific_file_copy_connectionString": {
						"value": "[parameters('lnk_src_specific_file_copy_connectionString')]"
					},
					"lnk_trg_speciffilecopy_1_connectionString": {
						"value": "[parameters('lnk_trg_speciffilecopy_1_connectionString')]"
					},
					"lnk_trg_specific_file_copy_accountKey": {
						"value": "[parameters('lnk_trg_specific_file_copy_accountKey')]"
					},
					"TrgGen2Storage_properties_typeProperties_url": {
						"value": "[parameters('TrgGen2Storage_properties_typeProperties_url')]"
					},
					"lnk_trg_specific_file_copy_properties_typeProperties_url": {
						"value": "[parameters('lnk_trg_specific_file_copy_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}